# OpenAI API Key (required)
OPENAI_API_KEY=your_openai_api_key_here

# Available models - comma-separated list
MODELS=o3-mini,gpt-4o,gpt-4o-mini

# Suffix label to display for OpenAI models in dropdown
OPENAI_SUFFIX=(OpenAI)

# Ollama API Endpoint (e.g., http://localhost:11434)
OLLAMA_ENDPOINT=http://localhost:11434

# Ollama API Key (if required, else leave blank)
OLLAMA_API_KEY=

# Available Ollama models - comma-separated list
OLLAMA_MODELS=llama2,phi3

# Suffix label to display for Ollama models in dropdown
OLLAMA_SUFFIX=(Ollama)

# Model to use for document summarization and automatic queries
SUMMARY_MODEL=gpt-4o-mini

# Log level: DEBUG, INFO, WARNING, ERROR, CRITICAL
LOG_LEVEL=WARNING
